<% for(var i = 0; i < entries.length; i++){%>
<tr>
    <td><%= i+1 %></td>
    <td><%= entries[i].purpose %></td>
    <td><%= entries[i].email %></td>
    <td><%= entries[i].username %></td>
    <td><%= entries[i].password %></td>
    <td><%= entries[i].status %></td>
    <td>
        <a href="/update-entry" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a href="#" class="btn border-shadow delete">
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<%}%>